{
    "manifest_version": 2,
    "name": "pebbleredir",
    "description": "Redirect to Rebble documentation from outdated Pebble",
    "version": "1.0",
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": ["background", "webRequest", "webRequestBlocking", "*://developer.pebble.com/*", "*://developer.getpebble.com/*"]
}
